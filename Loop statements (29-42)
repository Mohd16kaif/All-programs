#include <iostream>
using namespace std;

// wap to print hello world 5 times

void helloWorld5Times()
{
    for (int i = 1; i <= 5; i++)
        cout << "Hello World " << endl;
}

// wap to print natural numbers (1 to 10)

void naturalNumbers1to10()
{
    for (int i = 1; i <= 10; i++)
        cout << i << endl;
}

//wap to print natural numbers in reverse order (10 to 1)

void revNaturalNumbers10to1(){
    for (int i = 10; i>=1; i--){
        cout<<i<<endl;
    }
}

